<%= paginator.render do %>
  <nav class="pagination is-centered">
    <%= prev_page_tag %>
    <ul class="pagination-list">
      <% each_page do |page| %>
        <% if page.left_outer? || page.right_outer? || page.inside_window? %>
          <%= page_tag page %>
        <% elsif !page.was_truncated? %>
          <%= gap_tag %>
        <% end %>
      <% end %>
    </ul>
    <%= next_page_tag %>
  </nav>
<% end %>
